if [[ -e /tmp/runserver.pid ]]; then
  PIDS="$(ps -edf | grep $(cat /tmp/runserver.pid) | grep python | awk '{print $2}')"
  if [[ -n "${PIDS}" ]]; then
    kill ${PIDS}
    wait ${PIDS} 2>/dev/null
  fi
  rm -f /tmp/runserver.pid
fi
deactivate
unset deactivate
if type deactivate 2>/dev/null | head -n 1 | grep -q "function$" ; then
  deactivate
  unset deactivate
fi
if [ -n "${OLDPIP}" ]; then
  eval "$(echo ${OLDPIP} | base64 --decode)"
  unset OLDPIP
else
  unalias pip
fi
